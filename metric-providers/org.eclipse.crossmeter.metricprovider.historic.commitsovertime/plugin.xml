<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<plugin>
  <extension point="com.googlecode.pongo.runtime.osgi">
    <class name="org.eclipse.crossmeter.metricprovider.historic.commitsovertime.model.CommitsOverTime"/>
    <class name="org.eclipse.crossmeter.metricprovider.historic.commitsovertime.model.RepositoryData"/>
  </extension>
  <extension
        id="org.eclipse.crossmeter.metricprovider.historic.commitsovertime.CommitsOverTimeHistoricMetricProvider"
        point="org.eclipse.crossmeter.platform.metricprovider">
     <metricProvider
           provider="org.eclipse.crossmeter.metricprovider.historic.commitsovertime.CommitsOverTimeHistoricMetricProvider">
     </metricProvider>
  </extension>
  <extension
        point="org.eclipse.crossmeter.platform.visualisation.metric">
     <visualisation
           json="vis/commitsovertime.json">
     </visualisation>
  </extension>
</plugin>
