<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.crossmeter.platform.metricprovider">
      <metricProvider
            provider="org.eclipse.crossmeter.metricprovider.trans.githubstars.StarsTransientMetricProvider">
      </metricProvider>
   </extension>
   
   
   <extension point="com.googlecode.pongo.runtime.osgi">
    <class name="org.eclipse.crossmeter.metricprovider.trans.githubstars.model.Stars"/>
    <class name="org.eclipse.crossmeter.metricprovider.trans.githubstars.model.Stargazers"/>
  </extension>
   <extension
         point="org.eclipse.crossmeter.platform.visualisation.metric">
      <visualisation
            json="vis/trans.githubstas.json">
      </visualisation>
   </extension>
  
</plugin>
