<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="id1"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
            <run class="org.eclipse.crossmeter.rascal.test.RascalTestCaseGenerator"/>
      </application>
   </extension>
   <extension
         id="mweApp"
         name="Crossmeter MWE App"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="org.eclipse.crossmeter.rascal.test.MWEApplication">
         </run>
      </application>
   </extension>

</plugin>
